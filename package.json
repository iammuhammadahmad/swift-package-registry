{
  "name": "swift-package-registry",
  "version": "1.0.0",
  "private": true,
  "description": "A registry of Swift packages",
  "author": "twodayslate <zac@gorak.us>",
  "license": "ISC",
  "repository": "https://github.com/twodayslate/swift-package-registry.git",
  "homepage": "https://github.com/twodayslate/swift-package-registry",
  "bugs": "https://github.com/twodayslate/swift-package-registry/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@octokit/rest": "^17.11.2",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "dockerode": "^3.2.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-partials": "^0.3.0",
    "express-session": "^1.17.1",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "probot": "^10.8.0",
    "require-context": "^1.1.0",
    "sequelize": "^6.3.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.4",
    "smee-client": "^1.2.2",
    "standard": "^14.3.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
